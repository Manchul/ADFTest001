{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "ADFIncCopyTutorialDF003"
		},
		"AzureStorageLinkedService_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureStorageLinkedService'"
		},
		"ProcessedFile_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "processed_file.txt\n"
		},
		"ProcessedFile_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "adfdemo/stage"
		},
		"SourceFile_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "un_test_file.txt"
		},
		"SourceFile_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "adfdemo/rawdata"
		},
		"AzureStorageLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfdemo003sa.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureStorageLinkedService_accountKey": {
						"value": "[parameters('AzureStorageLinkedService_accountKey')]"
					},
					"AzureStorageLinkedService_properties_typeProperties_url": {
						"value": "[parameters('AzureStorageLinkedService_properties_typeProperties_url')]"
					},
					"ProcessedFile_properties_typeProperties_fileName": {
						"value": "[parameters('ProcessedFile_properties_typeProperties_fileName')]"
					},
					"ProcessedFile_properties_typeProperties_folderPath": {
						"value": "[parameters('ProcessedFile_properties_typeProperties_folderPath')]"
					},
					"SourceFile_properties_typeProperties_fileName": {
						"value": "[parameters('SourceFile_properties_typeProperties_fileName')]"
					},
					"SourceFile_properties_typeProperties_folderPath": {
						"value": "[parameters('SourceFile_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}